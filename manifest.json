{
    "manifest_version": 2,
    "name": "マウスストーカー",
    "description": "マウスに追従してきます.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
				"http://*/*",
				"https://*/*"
			],
            "js": [
				"vendor/jquery-2.2.4.js",
				"vendor/jquery.easing.1.3.js",
				"mouse-stalker.js"
			],
            "run_at": "document_start"
		}
	],
    "web_accessible_resources": [
		"image/*"
	]
}
